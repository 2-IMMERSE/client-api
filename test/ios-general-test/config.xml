<?xml version='1.0' encoding='utf-8'?>
<widget id="eu.immerse2.ios.general.test1" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>2-Immerse iOS General Test 1</name>
    <description>
        2-Immerse iOS General Test 1
    </description>
    <author href="http://2immerse.eu">
        2-Immerse
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="ws://*/*" />
    <allow-intent href="wss://*/*" />
    <hook src="scripts/pre-build.sh" type="before_build" />
    <hook src="scripts/pre-build.sh" type="before_run" />
    <preference name="LoadUrlTimeoutValue" value="-1" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-dialclient" spec="file:cordova-dialclient" />
    <plugin name="cordova-plugin-console" spec="^1.0.7" />
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-custom-config" spec="~3.0.14" />
</widget>
